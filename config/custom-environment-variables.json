{
  "openapiConfig": {
    "filePath": "OPENAPI_FILE_PATH"
  },
  "telemetry": {
    "serviceName": "TELEMETRY_SERVICE_NAME",
    "hostname": "TELEMETRY_HOST_NAME",
    "version": "TELEMETRY_SERVICE_VERSION",
    "logger": {
      "level": "LOG_LEVEL",
      "prettyPrint": {
        "__name": "LOG_PRETTY_PRINT_ENABLED",
        "__format": "boolean"
      }
    },
    "tracing": {
      "isEnabled": "TELEMETRY_TRACING_ENABLED",
      "url": "TELEMETRY_TRACING_URL"
    },
    "metrics": {
      "enabled": "TELEMETRY_METRICS_ENABLED",
      "url": "TELEMETRY_METRICS_URL",
      "interval": "TELEMETRY_METRICS_INTERVAL"
    }
  },
  "server": {
    "port": "SERVER_PORT",
    "request": {
      "payload": {
        "limit": "REQUEST_PAYLOAD_LIMIT"
      }
    },
    "response": {
      "compression": {
        "enabled": {
          "__name": "RESPONSE_COMPRESSION_ENABLED",
          "__format": "boolean"
        }
      }
    }
  },
  "app": {
    "externalIdTag": "APP_EXTERNAL_ID_TAG",
    "remote": {
      "api": {
        "baseUrl": "APP_REMOTE_API_BASE_URL",
        "xApiKey": "APP_REMOTE_API_X_API_KEY",
        "username": "APP_REMOTE_API_USERNAME",
        "password": "APP_REMOTE_API_PASSWORD",
        "timeoutMs": {
          "__name": "APP_REMOTE_API_TIMEOUT_MS",
          "__format": "number"
        }
      },
      "replication": {
        "baseUrl": "APP_REMOTE_REPLICATION_BASE_URL",
        "xApiKey": "APP_REMOTE_REPLICATION_X_API_KEY",
        "username": "APP_REMOTE_REPLICATION_USERNAME",
        "password": "APP_REMOTE_REPLICATION_PASSWORD",
        "timeoutMs": {
          "__name": "APP_REMOTE_REPLICATION_TIMEOUT_MS",
          "__format": "number"
        }
      }
    }
  }
}

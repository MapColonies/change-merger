{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "tracing": {
      "isEnabled": false
    },
    "shared": {},
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": 8080,
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "app": {
    "externalIdTag": "externalId",
    "remote": {
      "api": {
        "baseUrl": "https://api.openstreetmap.org/api/0.6/",
        "timeoutMs": 10000
      },
      "replication": {
        "baseUrl": "https://download.geofabrik.de/europe/andorra-updates/",
        "timeoutMs": 10000
      }
    }
  }
}
